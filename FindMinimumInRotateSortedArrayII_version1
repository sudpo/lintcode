class Solution {
public:
    /**
     * @param num: the rotated sorted array
     * @return: the minimum number in the array
     */
    int findMin(vector<int> &num) {
        // write your code here
        int result = INT_MAX;
        for(int i = 0; i < num.size(); ++i) {
            result = min(result, num[i]);
        }
        return result;
    }
};
